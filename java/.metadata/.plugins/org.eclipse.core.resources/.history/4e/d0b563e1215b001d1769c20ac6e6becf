package com.imooc.test;

import com.imooc.animal.Cat;
import com.imooc.animal.Dog;

public class Test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Cat one = new Cat();
		one.setName("小花猫");
		one.setSpecies("中华田园猫");
		one.eat();
		one.run();
		System.out.println("============");
		Dog two = new Dog();
		two.setName("牛牛");
		two.setMouth(1);
		two.sleep();
		two.eat();
		System.out.println("============");
	}

}
