package com.imooc.animal;

public class CatTest {
	public static void main(String[] args) {
		// 对象实例化
		Cat one = new Cat("花花");
		Cat two = new Cat();
		Cat three = new Cat("花花", 2, 10, "英国短毛猫");
		// 测试
		one.eat();
		one.run();
		one.month = 12;
		one.name = "花花";
		System.out.println("年龄" + one.month);
		System.out.println("名称" + one.name);
		System.out.println("品种" + one.species);
		System.out.println("体重" + one.weigth);
		one.run(one.name);
		System.out.println("年龄" + three.month);
		System.out.println("名称" + three.name);
		System.out.println("品种" + three.species);
		System.out.println("体重" + three.weigth);
	}
}
