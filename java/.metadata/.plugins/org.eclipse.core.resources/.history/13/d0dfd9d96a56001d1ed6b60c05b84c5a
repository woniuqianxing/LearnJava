package com.imooc.animal;

/**
 * 宠物猫类
 * 
 * @author W050000287
 *
 */
public class Cat {
	// 属性(成员属性)：昵称，年龄，体重，品种
	String name;
	int month;
	double weight;
	String species;

	public Cat() {
		System.out.println("我是无参构造方法");
	}

	public Cat(String name) {
		System.out.println("我是带参构造方法");
	}

	public Cat(String newName, int newMonth, double newWeight, String newSpecies) {
		name = newName;
		month = newMonth;
		weight = newWeight;
		species = newSpecies;

	}

	// 方法(成员方法)：跑动，吃东西
	public void run() {
		System.out.println("小猫会跑");
	}

	public void run(String name) {
		System.out.println(name + "小猫会跑");
	}

	public void eat() {
		System.out.println("小猫吃鱼");
	}

}
