package com.imooc.thread;

public class SyncSample {
	// 内部类，用来输出4个文字
	class Printer {
		public void print() {
			// 实现动画效果
			Thread.sleep(500);
			System.out.print("魑");
			Thread.sleep(500);
			System.out.print("魅");
			Thread.sleep(500);
			System.out.print("魍");
			Thread.sleep(500);
			System.out.print("魉");
			System.out.println();
		}
	}
}
